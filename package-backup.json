{
  "name": "cover_letter_builder",
  "version": "1.0.0",
  "private": true,
  "description": "Cover Letter React Builder",
  "scripts": {
    "lint": "eslint source/**/*.js source/**/*.jsx",
    "prebuild": "npm-run-all clean-dist css lccss rnacss clnjtcss ldejtcss wlbcss intl-css-prebuild zetycss bootstrap-cln bootstrap-clnjt bootstrap-lc bootstrap-rna bootstrap-luk bootstrap-lcfr bootstrap-lces bootstrap-lcit bootstrap-lcde bootstrap-lcpl bootstrap-zety",
    "build": "babel-node ./tools/build.js",
    "bootstrap-cln": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/cln ./source/stylesheets/cln/bootstrap_extends",
    "bootstrap-cln:watch": "npm run bootstrap-cln -- --watch",
    "bootstrap-clnjt": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/clnjt ./source/stylesheets/clnjt/bootstrap_extends",
    "bootstrap-clnjt:watch": "npm run bootstrap-clnjt -- --watch",
    "bootstrap-ldejt": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/ldejt ./source/stylesheets/ldejt/bootstrap_extends",
    "bootstrap-ldejt:watch": "npm run bootstrap-ldejt -- --watch",
    "bootstrap-lc": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/lca ./source/stylesheets/lca/bootstrap_extends",
    "bootstrap-lc:watch": "npm run bootstrap-lc -- --watch",
    "bootstrap-rna": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/rna ./source/stylesheets/rna/bootstrap_extends",
    "bootstrap-rna:watch": "npm run bootstrap-rna -- --watch",
    "bootstrap-luk": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/luk ./source/stylesheets/luk/bootstrap_extends",
    "bootstrap-luk:watch": "npm run bootstrap-luk -- --watch",
    "bootstrap-lcfr": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/lfr ./source/stylesheets/lfr/bootstrap_extends",
    "bootstrap-lcfr:watch": "npm run bootstrap-lcfr -- --watch",
    "bootstrap-lces": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/les ./source/stylesheets/les/bootstrap_extends",
    "bootstrap-lces:watch": "npm run bootstrap-lces -- --watch",
    "bootstrap-lcit": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/lit ./source/stylesheets/lit/bootstrap_extends",
    "bootstrap-lcit:watch": "npm run bootstrap-lcit -- --watch",
    "bootstrap-lcde": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/lde ./source/stylesheets/lde/bootstrap_extends",
    "bootstrap-lcde:watch": "npm run bootstrap-lcit -- --watch",
    "bootstrap-lcpl": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/lpl ./source/stylesheets/lpl/bootstrap_extends",
    "bootstrap-lcpl:watch": "npm run bootstrap-lcpl -- --watch",
    "bootstrap-zety": "node-sass --output-style compressed -o ./build/stylesheets/bootstrap/zty ./source/stylesheets/zety/bootstrap_extends",
    "bootstrap-zety:watch": "npm run bootstrap-rna -- --watch",
    "css": "node-sass --output-style compressed -o ./build/stylesheets/cln ./source/stylesheets/cln --source-map true",
    "css:watch": "npm run css -- --watch",
    "lccss": "node-sass --output-style compressed -o ./build/stylesheets/lca ./source/stylesheets/lca --source-map true",
    "lccss:watch": "npm run lccss -- --watch",
    "rnacss": "node-sass --output-style compressed -o ./build/stylesheets/rna ./source/stylesheets/rna --source-map true",
    "rnacss:watch": "npm run rnacss -- --watch",
    "clnjtcss": "node-sass --output-style compressed -o ./build/stylesheets/clnjt ./source/stylesheets/clnjt --source-map true",
    "clnjtcss:watch": "npm run clnjtcss -- --watch",
    "ldejtcss": "node-sass --output-style compressed -o ./build/stylesheets/ldejt ./source/stylesheets/ldejt --source-map true",
    "ldejtcss:watch": "npm run ldejtcss -- --watch",
    "wlbcss": "node-sass --output-style compressed -o ./build/stylesheets/wlb ./source/stylesheets/zety --source-map true",
    "lukcss": "node-sass --output-style compressed -o ./build/stylesheets/luk ./source/stylesheets/luk --source-map true",
    "lukcss:watch": "npm run lukcss -- --watch",
    "lcfrcss": "node-sass --output-style compressed -o ./build/stylesheets/lfr ./source/stylesheets/lfr --source-map true",
    "lcfrcss:watch": "npm run lcfrcss -- --watch",
    "lcescss": "node-sass --output-style compressed -o ./build/stylesheets/les ./source/stylesheets/les --source-map true",
    "lcescss:watch": "npm run lcescss -- --watch",
    "lcitcss": "node-sass --output-style compressed -o ./build/stylesheets/lit ./source/stylesheets/lit --source-map true",
    "lcitcss:watch": "npm run lcitcss -- --watch",
    "lcdecss": "node-sass --output-style compressed -o ./build/stylesheets/lde ./source/stylesheets/lde --source-map true",
    "lcdecss:watch": "npm run lcitcss -- --watch",
    "lcplcss": "node-sass --output-style compressed -o ./build/stylesheets/lpl ./source/stylesheets/lpl --source-map true",
    "lcplcss:watch": "npm run lcplcss -- --watch",
    "zetycss": "node-sass --output-style compressed -o ./build/stylesheets/zty ./source/stylesheets/zety --source-map true",
    "zetycss:watch": "npm run rnacss -- --watch",
    "prestart": "babel-node tools/startMessage.js",
    "intl-css-prebuild": "npm-run-all lukcss lcfrcss lcescss lcitcss lcdecss lcplcss ldejtcss",
    "intl-css-start": "npm-run-all --parallel lukcss lukcss:watch lcfrcss lcfrcss:watch lcescss lcescss:watch lcitcss litcss:watch lcdecss lcdecss:watch lcplcss lcplcss:watch ldejtcss:watch",
    "start": "npm-run-all --parallel node index run:server bootstrap-cln bootstrap-cln:watch bootstrap-clnjt bootstrap-clnjt:watch bootstrap-ldejt bootstrap-ldejt:watch bootstrap-lc bootstrap-lc:watch bootstrap-rna bootstrap-rna:watch bootstrap-luk bootstrap-luk:watch bootstrap-lcfr bootstrap-lcfr:watch bootstrap-lces bootstrap-lces:watch bootstrap-lcit bootstrap-lcit:watch bootstrap-lcde bootstrap-lcde:watch bootstrap-lcpl bootstrap-lcpl:watch bootstrap-zety bootstrap-zety:watch css css:watch lccss lccss:watch rnacss rnacss:watch clnjtcss clnjtcss:watch ldejtcss ldejtcss:watch wlbcss zetycss zetycss:watch build",
    "run:server": "webpack-dev-server --inline --config webpack.config.js --progress --colors --devtool source-map --hot",
    "clean-dist": "npm run remove-dist && mkdir build",
    "remove-dist": "rimraf ./build",
    "lint:watch": "npm run lint -- --watch",
    "stylelint": "stylelint \"source/stylesheets/**/*.{css,scss,sass}\"",
    "test": "jest"
  },
  "author": "Vivek Tripathi",
